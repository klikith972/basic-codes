class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int,int> u;
        int n=nums.size();
        for(int i=0;i<n;i++){
            int ans=target-nums[i];
            if(u.find(ans)!=u.end()){
                return {u[ans],i};
            }
            u[nums[i]]=i;
        }
        return {};
    }
};
